<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="393.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="helloGui.CardViewController">
   <ListView id="CardList" fx:id="CardList" layoutX="27.0" layoutY="61.0" onMouseClicked="#MC" prefHeight="300.0" prefWidth="200.0" />

   <Label layoutX="103.0" layoutY="44.0" prefHeight="17.0" prefWidth="48.0" text="Cards" />
   <ImageView id="CardPic" fx:id="CardPic" fitHeight="263.0" fitWidth="176.0" layoutX="262.0" layoutY="61.0" pickOnBounds="true" preserveRatio="true" />
   <Button layoutX="265.0" layoutY="337.0" mnemonicParsing="false" text="Add Card To :" />
   <MenuBar fx:id="MyMenu" layoutY="2.0">
     <menus>
       <Menu mnemonicParsing="false" text="My Decks">
         <items>
           <MenuItem mnemonicParsing="false" onAction="#launchDeckViewer" text="View Existing" />
               <MenuItem mnemonicParsing="false" onAction="#launchDeckAdder" text="Add New" />
         </items>
       </Menu>
       <Menu mnemonicParsing="false" text="Card Searcher">
         <items>
           <MenuItem mnemonicParsing="false" onAction="#launchCardSearch" text="Launch!" />
         </items>
       </Menu>
       <Menu mnemonicParsing="false" text="Help">
         <items>
           <MenuItem mnemonicParsing="false" text="About" />
         </items>
       </Menu>
     </menus>
   </MenuBar>
   <ComboBox layoutX="362.0" layoutY="337.0" prefWidth="150.0" />
</AnchorPane>
